{
  "name": "md-ltss-screening-tool",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --ext .js,.jsx --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "prepare": "husky",
    "precommit": "lint-staged",
    "prepush": "vitest run"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix"
  },
  "dependencies": {
    "@uswds/uswds": "^3.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.33.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "vitest": {
    "globals": true,
    "environment": "jsdom",
    "setupFiles": "./vitest.setup.js"
  }
}
